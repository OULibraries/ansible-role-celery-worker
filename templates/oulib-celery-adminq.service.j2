[Unit]
Description=OULibraries oulib-celery/adminq Service

[Service]
WorkingDirectory={{ celery_adminq_dir }}
ExecStart=/home/micromamba/{{ celery_adminq_user }}/envs/{{ mamba_env_name }}/bin/python /home/micromamba/{{ celery_adminq_user }}/envs/{{ mamba_env_name }}/bin/celery worker --queues {{ celery_adminq_queuename }} --hostname {{ celery_adminq_hostname }} --logfile {{ celery_adminq_log_dir }}/{{ celery_adminq_queuename }}.log --loglevel WARNING
User={{ celery_adminq_user }}

[Install]
WantedBy=multi-user.target
Alias=oulib-celery-adminq.service
