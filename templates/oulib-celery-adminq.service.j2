[Unit]
Description=OULibraries oulib-celery/adminq Service

[Service]
WorkingDirectory={{ celery_adminq_dir }}
ExecStart=/home/micromamba/{{ celery_adminq_user }}/envs/dev_py39/bin/python celery worker --queues {{ celery_adminq_queuename }} --hostname {{ celery_adminq_hostname }} --logfile {{ celery_adminq_log_dir }}/{{ celery_adminq_queuename }}.log --loglevel WARN
User={{ celery_adminq_user }}

[Install]
WantedBy=multi-user.target
Alias=oulib-celery-adminq.service
